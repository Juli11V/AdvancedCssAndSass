{
  "name": "nexter",
  "version": "1.0.0",
  "description": "Landing page for nexter",
  "main": "index.js",
  "scripts": {
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/main.css -o css/main.prefix.css",
    "compress:css": "node-sass css/main.prefix.css css/main.css --output-style compressed",
    "build:css": "npm-run-all  prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juli11V/AdvancedCssAndSass.git"
  },
  "author": "Julia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Juli11V/AdvancedCssAndSass/issues"
  },
  "homepage": "https://github.com/Juli11V/AdvancedCssAndSass#readme",
  "devDependencies": {
    "node-sass": "^8.0.0",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21"
  }
}
